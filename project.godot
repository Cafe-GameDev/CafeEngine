; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CafeEngine"
config/features=PackedStringArray("4.5", "GL Compatibility")

[audio]

buses/default_bus_layout="uid://iqdcw3igji8l"

[autoload]

CoreEngine="*res://addons/core_engine/utils/core_engine.gd"
AudioManager="*res://addons/audio_manager/components/audio_manager.tscn"
DataBehavior="*res://addons/data_behavior/components/data_behavior.gd"
StateMachine="*res://addons/state_machine/components/state_machine.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/audio_manager/plugin.cfg", "res://addons/core_engine/plugin.cfg", "res://addons/data_behavior/plugin.cfg", "res://addons/state_machine/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
